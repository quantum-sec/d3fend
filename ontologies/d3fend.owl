    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <d3f:definition>Comparing the cryptographic hash or derivative of a pointer's value to an expected value.</d3f:definition>
    <rdfs:subClassOf rdf:resource="#ApplicationHardening"/>
    <rdfs:subClassOf rdf:nodeID="N42012ae6b59e4e48992dc66dd56d6fe8"/>
