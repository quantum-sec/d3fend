    <d3f:kb-is-example-of rdf:resource="#ProcessLineageAnalysis"/>
    <rdfs:label>Reference - CAR-2014-05-002: Services launching Cmd -</rdfs:label>
    <d3f:kb-organization></d3f:kb-organization>
  </rdf:Description>
