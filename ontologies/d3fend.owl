    <rdfs:subClassOf rdf:nodeID="N4916c88b332c443a9b41ace77456ccf1"/>
    <rdfs:label>URL Analysis</rdfs:label>
    <d3f:d3fend-id>D3-UA</d3f:d3fend-id>
  </rdf:Description>
