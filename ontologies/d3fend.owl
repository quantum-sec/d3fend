    <d3f:spoofs rdf:resource="#IntranetNetwork"/>
    <rdf:type rdf:resource="#DecoyEnvironment"/>
    <d3f:definition>An environment created for the purpose of attracting attackers and eliciting their behaviors that is not connected to any production enterprise systems.</d3f:definition>
    <d3f:enables rdf:resource="#Deceive"/>
