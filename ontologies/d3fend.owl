  <rdf:Description rdf:about="#IPCTrafficAnalysis">
    <rdfs:label>IPC Traffic Analysis</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdfs:subClassOf rdf:nodeID="N793bbb590e614a55a007c0f8f0ed9d92"/>
    <d3f:kb-reference rdf:resource="#Reference_CAR-2013-01-003:SMBEventsMonitoring_"/>
